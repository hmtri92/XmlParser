			scope="request"
			validate="false"
			parameter="dispatch">
			<forward name="searchApp" path="/applications/applicationdetails/AppSearch.jsp"/>
			<forward name="listApp" path="/applications/applicationdetails/AppList.jsp"/>
		</action>

		<action 
			path="/AppLicenceParticularAction"
			type=".action.applications.applicationdetails.AppLicenceParticularAction"
			name="AppLicenceParticularAddEdit"
			scope="request"
			validate="false"
			parameter="dispatch">
			<forward name="addAppLicence" path="/applications/applicationdetails/AppLicenceParticularAddEdit.jsp"/>
			<forward name="addTACAppLicence" path="/applications/applicationdetails/AppTACLicenceParticular.jsp"/>
			<forward name="success" path="/AppSearchAction.do?dispatch=listAppDetails"/>
			<forward name="savesuccess" path="/common/CommonSuccess.jsp"/>
			<forward name="failure" path="/AppSearchAction.do?dispatch=listAppDetails"/>
			<forward name="viewAppLicPrintInfo" path="/applications/applicationdetails/AppLicencePrintInfo.jsp"/>
			<forward name="sendApprovalEmailSuccess" path="/common/CommonSuccess.jsp"/>
		</action>
		
